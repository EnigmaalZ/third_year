<Window x:Class="BarreDeChargement.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BarreDeChargement"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="550">
    <Grid>
        <local:CustomProgressBar x:Name="maProgressBar" Alert="TestDepassementSeuil" Minimum="0" Value="50" Maximum="100" AlertLevel="75"  HorizontalAlignment="Left" Height="50" Margin="75,150,0,75" VerticalAlignment="Top" Width="370" />
        <Button Name="plus" Content="plus" HorizontalAlignment="Left" Margin="465,150,0,25" VerticalAlignment="Top" Width="50" Height="50" Click="Button_Plus"/>
        <Button Name="moins" Content="moins" HorizontalAlignment="Left" Margin="5,150,0,0" VerticalAlignment="Top" Width="50" Height="50" Click="Button_Moins"/>

    </Grid>
</Window>
